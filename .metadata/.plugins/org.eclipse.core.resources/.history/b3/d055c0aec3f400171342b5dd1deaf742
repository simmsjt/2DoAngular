angular.module('appModule')
.filter('incomplete',function(){
	return function(todos, showCompleted){
		var results = [];
		todos.forEach(function(todo){
			if(showCompleted === true){
				results.push(todo);
			} else{
				if(todo.completed === false){
				results.push(todo);
				}
			}
		});
		return results;
	}
	
})