angular.module('authModule')
.factory('authService', function($http, $cookies, $location){
	var service = {};
	
	service.getToken = function(){
		$cookies.get('uid');
	}
	
	service.register = function(user){
		return $http({
			method:'POST',
			url: 'rest/auth/register',
			headers ; {
				'Content-Type' : 'application/json'
			},
			data : user
		})
	}
	
	return service;
})
